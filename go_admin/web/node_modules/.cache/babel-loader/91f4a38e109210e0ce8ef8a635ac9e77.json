port infoList from \"@/mixins/infoList\";\r\nimport { exportExcel, loadExcelData, downloadTemplate } from \"@/api/excel\";\r\nimport { getMenuList } from \"@/api/menu\";\r\nexport default {\r\n  name: 'Excel',\r\n  mixins: [infoList],\r\n  data() {\r\n    return {\r\n      listApi: getMenuList,\r\n      path: path\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapGetters('user', ['userInfo', 'token'])\r\n  },\r\n  methods: {\r\n    handleExcelExport(fileName) {\r\n      if (!fileName || typeof fileName !== \"string\") {\r\n        fileName = \"ExcelExport.xlsx\";\r\n      }\r\n      exportExcel(this.tableData, fileName);\r\n    },\r\n    loadExcel() {\r\n      this.listApi = loadExcelData;\r\n      this.getTableData();\r\n    },\r\n    downloadExcelTemplate() {\r\n      downloadTemplate('ExcelTemplate.xlsx')\r\n    }\r\n  },\r\n  created() {\r\n    this.pageSize = 999;\r\n    this.getTableData();\r\n  }\r\n}\r\n</script>"],"sourceRoot":"src/view/example/excel"}]}